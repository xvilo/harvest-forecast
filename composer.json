{
  "name": "xvilo/harvest-forecast",
  "type": "library",
  "description": "Harvest Forecast API client",
  "homepage": "https://github.com/xvilo/harvest-forecast",
  "keywords": ["forecast", "harvest forecast", "harevest", "forcast", "api", "client"],
  "license": "MIT",
  "authors": [
    {
      "name": "Sem Schilder",
      "homepage": "http://sem.design"
    }
  ],
  "require": {
    "php": "^7.2",
    "psr/http-message": "^1.0",
    "psr/cache": "^1.0",
    "php-http/httplug": "^1.1 || ^2.0",
    "php-http/discovery": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/client-common": "^1.6 || ^2.0",
    "php-http/cache-plugin": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0",
    "php-http/guzzle6-adapter": "^1.0 || ^2.0",
    "php-http/mock-client": "^1.2",
    "guzzlehttp/psr7": "^1.2",
    "cache/array-adapter": "^0.4",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": { "Harvest\\Forecast\\": "src/" }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
